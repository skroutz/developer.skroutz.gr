        <%= link_to t('view_doc'), '/analytics/', class: 'goto' %>
      </div>
    </div>
  </div>

  <div class="doc-panel-wrapper">
    <div class="doc-panel">
      <div class="doc-icon"><i class="fa-heart"></i></div>
      <div class="doc-body">
        <h3 class="heading"><%= t('titles.widgets') %></h3>

        <h3 class="heading"><%= t('titles.analytics') %></h3>
        <p class="description"><%= t('docs.analytics.short_description') %></p>
        <%= link_to t('view_doc'), '/analytics/', class: 'goto' %>
      </div>
    </div>
  </div>

  <div class="doc-panel-wrapper">
    <div class="doc-panel">
      <div class="doc-icon"><i class="fa-flash"></i></div>
